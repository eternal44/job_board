<%= form_for @profile do |f| %>
  <div class="form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
  </div>

  <div class="form-group">
    <%= f.label :phone_number %><br>
    <%= f.text_field :phone_number %><br>
  </div>

  </br>

  <div class="actions">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>

