<div class="container">
  <!-- Header -->
  <header id="top" class="header">
    <div class="text-center">
      <h1>Denarius</h1>
      <h3>A transparent place for job seekers & employers</h3>
      <br>
      <a href="/users/sign_in" class="btn btn-warning btn-lg">User Login</a>
      <div class"row">
        <%= link_to "New User Registration", new_user_registration_path %>
      </div>
    </div>
  </header>
  <hr>
  <section id="services" class=" services bg-primary">
    <div class="container">
      <div class="row text-center">
        <h2>The Denarius Difference</h2>
        <hr class="small">
      </div>
      <div class="row text-center col-lg-offset-2">
        <div class="col-md-3 col-sm-6">
          <div class="service-item">
            <span class="fa-stack fa-4x">
              <i class="fa fa-circle fa-stack-2x"></i>
              <i class="fa fa-cloud fa-stack-1x text-primary"></i>
            </span>
            <h4>
              <strong>Long History</strong>
            </h4>
            <p>Review your worker's work history before you hire them.</p>
            <a href="#" class="btn btn-light">Learn More</a>
          </div>
        </div>
        <div class="col-md-3 col-sm-6">
          <div class="service-item">
            <span class="fa-stack fa-4x">
              <i class="fa fa-circle fa-stack-2x"></i>
              <i class="fa fa-flask fa-stack-1x text-primary"></i>
            </span>
            <h4>
              <strong>Chemistry</strong>
            </h4>
            <p>Request a specific worker you prefer.</p>
            <a href="#" class="btn btn-light">Learn More</a>
          </div>
        </div>
        <div class="col-md-3 col-sm-6">
          <div class="service-item">
            <span class="fa-stack fa-4x">
              <i class="fa fa-circle fa-stack-2x"></i>
              <i class="fa fa-shield fa-stack-1x text-primary"></i>
            </span>
            <h4>
              <strong>Pure Intent</strong>
            </h4>
            <p>100% of what you pay goes to the worker.</p>
            <a href="#" class="btn btn-light">Learn More</a>
          </div>
        </div>
      </div><!--row -->
    </div>
    <!-- /.container -->
  </section>
  <!-- Call to Action -->
  <aside class="call-to-action">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center">
          <div class="row">
            <div class="col-sm-3">
              <img class="img-circle img-responsive img-center" src="http://placehold.it/300x300" alt="">
              <h2>Cleaning</h2>
              <p>Professional cleaners that have gone through our training program.</p>
            </div>
            <div class="col-sm-3">
              <img class="img-circle img-responsive img-center" src="http://placehold.it/300x300" alt="">
              <h2>Landscaping & Carpentry</h2>
              <p>Gardening and furniture building or repairs.</p>
            </div>
            <div class="col-sm-3">
              <img class="img-circle img-responsive img-center" src="http://placehold.it/300x300" alt="">
              <h2>Appliance Installations & Repairs</h2>
              <p>Need help mounting a TV or putting in a refrigerator?</p>
            </div>
            <div class="col-sm-3">
              <img class="img-circle img-responsive img-center" src="http://placehold.it/300x300" alt="">
              <h2>Construction & Demolition</h2>
              <p>Pick from our long list of skilled workers.</p>
            </div>
          </div><!-- /.row -->
          <div class="well">
            <h2>Sign in to see all available tasks</h2>
            <a href="/users/sign_up" class="btn btn-lg btn-success">Sign Up Here</a>
          </div>
        </div>
